<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="db.LetvConfigEntity" table="letv_config" schema="main">
        <id name="configId">
            <column name="config_id" sql-type="integer(11)"/>
        </id>
        <property name="configMessageText">
            <column name="config_message_text" sql-type="text(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>