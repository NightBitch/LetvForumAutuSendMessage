<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>


    <class name="db.LetvLinkEntity" table="letv_link" schema="main">
        <id name="letvLinkId">
            <column name="letv_link_id" sql-type="integer(11)"/>
        </id>
        <property name="letvLinkName">
            <column name="letv_link_name" sql-type="text(40)" length="40"/>
        </property>
        <property name="letvLink">
            <column name="letv_link" sql-type="text(50)" length="50"/>
        </property>
        <property name="letvLastTime">
            <column name="letv_last_time" sql-type="text"/>
        </property>
        <property name="letvStatus">
            <column name="letv_status" sql-type="text(10)" length="10"/>
        </property>
        <property name="letvModuleId">
            <column name="letv_module_id" sql-type="integer(11)"/>
        </property>
    </class>
</hibernate-mapping>